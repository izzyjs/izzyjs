{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["typecheck","^build", "postbuild"],
      "outputs": ["dist/**"]
    },
    "typecheck": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "postbuild": {},
    "test": {
      "dependsOn": ["typecheck", "^test"],
      "outputs": ["coverage/**"]
    }
  }
}